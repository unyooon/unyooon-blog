{
  "editor.tabSize": 2,
  "path-autocomplete.pathMappings": {
    "~": "${folder}",
    "@": "${folder}"
  },
  "eslint.validate": [
    {
      "language": "vue",
      "autoFix": true
    },
    {
      "language": "javascript",
      "autoFix": true
    },
    {
      "language": "typescript",
      "autoFix": true
    }
  ],
  "eslint.autoFixOnSave": true,
  "editor.formatOnSave": false,
  "files.associations": {
    ".json": "jsonc"
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "cSpell.words": [
    "vuetify"
  ]
}
